    <table border="1">
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Descripci√≥n</th>
            <th colspan="2">Acciones</th>
        </tr>
        <% records.forEach(function(proyecto) { %>
        <tr>
            <td><%= proyecto._id %></td>
            <td><%= proyecto.nombre %></td>
